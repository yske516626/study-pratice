#include<stdio.h>
int Add(int a, int n)
{
	int total = a;
	int i = 0;
	int j = a;
	for(i = 1; i < n; i++)
	{
		a = 10 * a + j;
		total += a;
	}
	return total;
}
int fn(int a, int n)
{
	int j = a;
	int i = 0;
	for (i = 1; i < n; i++)
	{
		a = 10 * a + j;
	}
	return a;
}



int main(void)
{
	int a = 0;
	int n = 0;
	printf("input a and n :");
	scanf("%d%d", &a, &n);
	int ret = Add(a, n);
	printf("%d\n", ret);
	printf("%d\n", fn(a,n));
	return 0;
}








