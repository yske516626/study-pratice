#include<stdio.h>
#include<string.h>
int is_left_move(char* s1, char* s2)
{
int len1 = strlen(s1);
int len2 = strlen(s2);
if(len1 == len2)
{
strncat(s1, s2, len);
char* ret = strstr(s1, s2);
if (ret == NULL)
{
return 0;
}
else
{
return 1;
}
}
}
int main(void)
{
char arr1[30] = "abcdef";
char arr2[] = "cdefab";
int ret = is_left_move(arr1, arr2);
if (ret == 1)
{
printf("yes\n");
}
else
{
printf("no\n");
}
return 0;
}
